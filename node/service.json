{
  "memory": 512,
  "ttl": 60,
  "timeout": 5000,
  "minReplicas": 2,
  "maxReplicas": 4,
  "routes": {
    "returnRequests": {
      "path": "/_v/return-request",
      "public": true
    },
    "returnRequest": {
      "path": "/_v/return-request/:requestId",
      "public": true
    },
    "exportRequests": {
      "path": "/_v/return-request/export",
      "public": true
    },
    "settings": {
      "path": "/_v/returns/settings",
      "public": true
    },
    "sellerSetting": {
      "path": "/_v/returns/seller/settings",
      "public": true
    },
    "sellerSettings": {
      "path": "/_v/returns/seller/settings/:sellerId",
      "public": true
    },
    "orders": {
      "path": "/_v/returns/seller/orders",
      "public": true
    },
    "giftcard": {
      "path": "/_v/returns/seller/giftcard",
      "public": true
    },
    "orderSummary": {
      "path": "/_v/return-request/orders/:orderId/summary",
      "public": true
    }
  }
}
