{
  "properties": {
    "orderId": {
      "type": "string"
    },
    "orderValue": {
      "type": "integer"
    },
    "refundable": {
      "type": "object",
      "properties": {
        "available": {
          "type": "integer"
        },
        "shipping": {
          "type": "integer"
        }
      },
      "required": ["available", "shipping"]
    },
    "refunds": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": ["goodwill", "prerefund", "refund"]
          },
          "status": {
            "type": "string",
            "enum": ["pending", "applied", "rejected"]
          },
          "value": {
            "type": "integer"
          }
        },
        "required": ["id", "type", "status", "value"]
      }
    }
  },
  "required": ["orderId", "orderValue", "refundable", "refunds"],
  "v-cache": false,
  "v-default-fields": ["id", "createdIn"],
  "v-indexed": ["id", "orderId"],
  "v-immediate-indexing": true
}
